{
    "multipart": [
        {
            "apply": {
                "model": "alexscaves:block/metal_rebar_center"
            }
        },
        {
            "when": {
                "connect_x": true
            },
            "apply": {
                "model": "alexscaves:block/metal_rebar_edges",
                "y": 90
            }
        },
        {
            "when": {
                "connect_y": true
            },
            "apply": {
                "model": "alexscaves:block/metal_rebar_edges",
                "x": 90
            }
        },
        {
            "when": {
                "connect_z": true
            },
            "apply": {
                "model": "alexscaves:block/metal_rebar_edges"
            }
        }
    ]
}